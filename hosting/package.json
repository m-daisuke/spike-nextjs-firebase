{
  "name": "hosting",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "14"
  },
  "scripts": {
    "dev": "NODE_ENV=development next src/",
    "build:dev": "NODE_ENV=development next build src/ && next export",
    "build:prod": "NODE_ENV=production next build src/ && next export",
    "start": "yarn build:prod && serve ./out",
    "serve:": "yarn build:dev && firebase emulators:start --only hosting",
    "lint": "next lint",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@material-ui/core": "^4.12.3",
    "@mui/material": "^5.0.0",
    "firebase-functions": "^3.15.6",
    "next": "11.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.15.4"
  },
  "devDependencies": {
    "@types/react": "17.0.19",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.3.2",
    "serve": "^12.0.1",
    "typescript": "4.4.3"
  }
}
